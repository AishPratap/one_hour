<p>import java.io.*;</p>
<p>/* Name of the class has to be &#8220;Main&#8221; only if the class is public. */</p>
<p>class Ideone</p>
<p>{</p>
<p> public static void main (String[] args) throws java.lang.Exception</p>
<p> {</p>
<p> //System.out.println(isMatchMain(&#8220;.&#8221;,&#8221;a&#8221;));</p>
<p> System.out.println(isMatchMain(&#8220;abc*bcd&#8221;, &#8220;abcdhghgbcd&#8221;));</p>
<p> }</p>
<p> public static boolean isMatchMain(String P, String S) {</p>
<p> if(P == null || S == null || P.length() == 0 || S.length() == 0) return false;</p>
<p> return isMatch( P, S);</p>
<p> }</p>
<p> public static boolean isMatch(String P, String S) {</p>
<p> if(P.length() == 0 &amp;&amp; S.length() == 0) {</p>
<p> return true;</p>
<p> }</p>
<p> if(S.length() == 0 &amp;&amp; !P.equals(&#8220;*&#8221;)) return false;</p>
<p> if(isCharEqual(&#8216;.&#8217;, P) || equalsString(P, S)) {</p>
<p> return isMatch(P.substring(1), S.substring(1));</p>
<p> }</p>
<p> if(isCharEqual(&#8216;*&#8217;, P)) {</p>
<p> return isMatch(P.substring(1), S) || isMatch(P, S.substring(1));</p>
<p> }</p>
<p> return false;</p>
<p> }</p>
<p> public static boolean isCharEqual(char ch, String S) {</p>
<p> if(S.length() &gt; 0 &amp;&amp; ch == S.charAt(0)) return true;</p>
<p> return false;</p>
<p> }</p>
<p> public static boolean equalsString(String P, String S) {</p>
<p> if(P.length() &gt; 0 &amp;&amp; S.length() &gt; 0 &amp;&amp; S.charAt(0) == P.charAt(0)) return true;</p>
